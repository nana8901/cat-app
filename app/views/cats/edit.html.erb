<h3>edit page</h3>


<form action="/cats/<%= @cat.id %>" method="post">
  <input name="_method" type="hidden" value="patch" />
  <p>Breed: <input type="text" name="breed" value="<%= @cat.breed %>">
  <p>Name: <input type="text" name="name" value="<%= @cat.name %>">
  <p>Image: <input type="text" name="image_url" value="<%= @cat.image_url %>">
  <p>age: <input type="text" name="age" value="<%= @cat.age %>">
  <input type="submit">
</form>